{
  "extends": [
    "react-app",
    "plugin:jsx-a11y/recommended"
  ],
  "plugins": [
    "jsx-a11y"
  ],
  "globals": {
    "axios": true,
    "@": true,
    "ssLog": true
  },
  "rules": {
    "jsx-a11y/no-autofocus": "warn",
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx",
          ".tsx"
        ]
      }
    ],
    "@typescript-eslint/explicit-function-return-type": 0
  }
}
